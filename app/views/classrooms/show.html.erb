<div class="container">
	<br><br>
	<h1>Classroom View</h1>
	<br>
	<b>Name: </b><%= @classroom.name %><br>
	<b>Description: </b><%= @classroom.description %><br>
	<br>
	<ul>
		<% @classroom.profiles.each do |profile| %>

				<li><b>Name:</b> <%= profile.fname %> <%= profile.lname %> <b>Ethnicity:</b> <%= profile.ethnicity %> <b>IEP</b>: <%= profile.iep %> <b>Math</b>: <%= profile.math %> <b>ELA:</b> <%= profile.ela %></li>

		<% end %>
	</ul>
	<p><%= link_to "Return to classrooms", classrooms_path %></p>
</div>
